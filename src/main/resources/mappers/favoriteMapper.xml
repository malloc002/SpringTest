<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.malloc.spring.test.ajax.dao.FavoriteDAO">
  	
  	<select id="selectFavoriteList" resultType="com.malloc.spring.test.ajax.model.Favorite">
  		SELECT
  			`id`
  			, `name`
  			, `url`
  			, `createdAt`
  			, `updatedAt`
  		FROM
  			`favorite`
  	</select>
  	
</mapper>